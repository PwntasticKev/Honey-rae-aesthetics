{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--stdio",
        "--browser=chromium",
        "--headless"
      ],
      "transport": "stdio",
      "timeoutMs": 120000
    },
    "serena": {
      "command": "npx",
      "args": ["-y", "<PKG>@latest"],
      "transport": "stdio",
      "timeoutMs": 120000,
      "autoApprove": []
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "<PKG>@latest"],
      "transport": "stdio",
      "timeoutMs": 120000,
      "autoApprove": []
    }
  }
}
